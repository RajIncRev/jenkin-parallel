pipeline {
  agent none
  stages {
    stage('build and run') {
      parallel {
        stage('linux-armv6') {
          agent none
          stages {
            stage('build') {
              steps {
                echo "Build application-1 in Darwin agent"
             	echo "Build application"
			 	bat "mvnw  -Dmaven.test.skip=true clean compile package"
              }
            }
            stage('run') {
              steps {
                 echo "Run application-1 in windows agent"
              }
            }
          }
        }      
      }
    }
  }
}